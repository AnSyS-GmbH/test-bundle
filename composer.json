{
    "name": "ansys/test-bundle",
    "type": "contao-bundle",
    "description": "Test",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "AnSyS GmbH",
            "homepage": "https://github.com/AnSyS-GmbH"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "^4.8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "extra": {
        "contao-manager-plugin": "AnSyS\\TestBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "AnSyS\\TestBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnSyS\\TestBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/AnSyS-GmbH/test-bundle/issues",
        "source": "https://github.com/AnSyS-GmbH/test-bundle"
    }
}
